        FileInputStream fis = new FileInputStream(f);
        byte[] bytes = new byte[(int) f.length()];
        fis.read(bytes);
        int dummyCount = 0;

        long readStart = System.nanoTime();                                                 
        for(int i = 0; i &lt; 100; i++){       
            PathValueSet.Builder bldpvs = PathValueSet.newBuilder();
            bldpvs.mergeFrom(bytes,0,bytes.length);
            PathValueSet pvs = bldpvs.build();
            dummyCount += pvs.getPathvaluesCount();
        }
        long readTotal = System.nanoTime() - readStart;
        System.out.println(String.valueOf( (readTotal / 1000000)) + ""ms"");  


        long readStartSecondRound = System.nanoTime();                                              
        for(int i = 0; i &lt; 1; i++){     
            PathValueSet.Builder bldpvs = PathValueSet.newBuilder();
            bldpvs.mergeFrom(bytes,0,bytes.length);
            PathValueSet pvs = bldpvs.build();
            dummyCount += pvs.getPathvaluesCount();

        }
        long readTotalSecondRound = System.nanoTime() - readStartSecondRound;
        System.out.println(String.valueOf( (readTotalSecondRound / 1000000)) + ""ms"");
        System.out.println(dummyCount);     

    }

}
